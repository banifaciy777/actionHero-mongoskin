{
  "name": "actionhero-mongoskin",
  "version": "0.0.1",
  "description": "MongoDB initializers for actionHero backed by MongoSkin",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/.bin/actionhero start",
    "startCluster": "node ./node_modules/.bin/actionhero startCluster",
    "actionHero": "node ./node_modules/.bin/actionhero",
    "help": "node ./node_modules/.bin/actionhero help",
    "test": "node ./node_modules/.bin/mocha --reporter spec"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/panjiesw/actionHero-mongoskin"
  },
  "keywords": [
    "actionhero",
    "initializer",
    "mongoskin",
    "mongodb"
  ],
  "author": "Panjie SW <panjie@panjiesw.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/panjiesw/actionHero-mongoskin/issues"
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "actionhero": "~7.0.0",
    "mongoskin": "~0.6.1",
    "validator": "~2.0.0"
  },
  "devDependencies": {
    "coffee-script": "~1.6.3",
    "which": "~1.0.5",
    "should": "~2.1.0",
    "mocha": "~1.14.0",
    "request": "~2.27.0"
  }
}
